<%- include('common') -%>
<div class="br-pagebody">
  <div class="card">
    <div class="card-header">
      <div class="card-title">
        <div class="title"><%=pageTitle%>列表</div>
      </div>
    </div>
    <div class="card-body">
      <table class="table">
        <thead>
          <tr>
            <th>#</th>
            <th><%=pageTitle%>名称</th>
            <th>创建时间</th>
            <th>最后一次修改时间</th>
            <th>操作</th>
          </tr>
        </thead>
        <tbody>
          <% for(var i=0;i<info.length;i++){ %>
          <%var data=info[i];%>
            <tr data-id="<%=data.id%>">
              <th scope="row">
                <%=i+1  %>
              </th>
              <td>
                <%= data.name %>
              </td>
              <td>
                <%= data.timeCreate %>
              </td>
              <td>
                <%= data.timeCreate %>
              </td>
              <td>
                <a href="javascript:void(0);" class="btn btn-teal btn-block disabled">修改</a>
                <a href="javascript:void(0);" class="btn btn-dark btn-block disabled" >删除</a>
              </td>
            </tr>
            <% } %>
        </tbody>
      </table>
    </div>
  </div>
</div>
<%- include('footer') -%>