<%- include('common') -%>

  <!-- Main Content -->
  <div class="side-body">
    <div class="page-title">
      <span class="title">分类列表</span>
      <div class="description">文章分类 默认10个每页</div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        <div class="card">
          <div class="card-header">
            <div class="card-title">
              <div class="title">Table</div>
            </div>
          </div>
          <div class="card-body">
            <table class="table">
              <thead>
                <tr>
                  <th>#</th>
                  <th>分类名称</th>
                  <th>创建时间</th>
                  <th>最后一次修改时间</th>
                  <th>操作</th>
                </tr>
              </thead>
              <tbody>
                <% for(var i=0;i<art.length;i++){ %>
                  <tr>
                    <th scope="row">
                      <%=i+1  %>
                    </th>
                    <td>
                      <a href=<%="/admin/typelist?_id=" +art[i]._id %> >
                        <%= art[i].type_name %>
                      </a>
                    </td>
                    <td>
                      <%= art[i].create_time %>
                    </td>
                    <td>
                      <%= art[i].update_time %>
                    </td>
                    <td>
                      <a href="javascript:void(0);" class="btn btn-info">修改</a>
                      <a href="javascript:void(0);" class="btn btn-danger">删除</a>
                    </td>
                  </tr>
                  <% } %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>


  <%- include('footer') -%>