<%- include('common') -%>
  <div class="side-body">
    <div class="page-title">
      <span class="title"><%=pageTitle%></span>
      <div class="description">显示文章列表 默认一页10篇文章</div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        <div class="card">
          <div class="card-header">
            <div class="card-title">
              <div class="title">Table</div>
            </div>
          </div>
          <div class="card-body">
            <table class="table">
              <thead>
                <tr>
                  <th>#</th>
                  <th>文章标题</th>
                  <th>分类</th>
                  <th>阅读人数</th>
                  <th>创建时间</th>
                  <th>最后一次修改时间</th>
                  <th>操作</th>
                </tr>
              </thead>
              <tbody>
              </tbody>
            </table>
            <nav aria-label="Page navigation" class="navigation"></nav>
            <!-- 模态框（Modal） -->
            <div class="modal fade" id="delArcModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
              <div class="modal-dialog">
                <div class="modal-content">
                  <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title" id="myModalLabel">删除此文章</h4>
                  </div>
                  <div class="modal-body">此文章将移动到回收站，是否继续？！</div>
                  <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">否</button>
                    <button type="button" class="btn btn-primary" data-dismiss="modal" id="delArc_btn">是</button>
                  </div>
                </div>
                <!-- /.modal-content -->
              </div>
              <!-- /.modal -->
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <%- include('footer') -%>
    <script src="/js/back/artlistTrash.js"></script>